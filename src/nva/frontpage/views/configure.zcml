<configure
    xmlns="http://namespaces.zope.org/zope"
    xmlns:browser="http://namespaces.zope.org/browser"
    xmlns:plone="http://namespaces.plone.org/plone"
    i18n_domain="nva.frontpage">

  -*- extra stuff goes here -*-

  <browser:page
    name="leftimageview"
    for="Products.CMFCore.interfaces.IFolderish"
    class=".leftimageview.Leftimageview"
    template="leftimageview.pt"
    permission="zope2.View"
    />

  <browser:page
    name="textfieldview"
    for="Products.CMFCore.interfaces.IFolderish"
    class=".textfieldview.Textfieldview"
    template="textfieldview.pt"
    permission="zope2.View"
    />

  <browser:page
    name="jumbotronview"
    for="*"
    class=".jumbotronview.Jumbotronview"
    template="jumbotronview.pt"
    permission="zope2.View"
    />

  <browser:page
    name="view"
    for="nva.frontpage.content.landingpage.ILandingpage"
    class=".landing_view.LandingView"
    template="landing_view.pt"
    permission="zope2.View"
    />

</configure>
